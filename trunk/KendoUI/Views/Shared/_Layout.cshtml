<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")
        @Scripts.Render("~/bundles/modernizr")*@

    @Styles.Render("~/KendoLib/styles/kendo.common.min.css")

    @Styles.Render("~/KendoLib/styles/kendo.default.min.css")
    @Styles.Render("~/KendoLib/styles/kendo.dataviz.min.css")

    @Styles.Render("~/KendoLib/styles/kendo.dataviz.default.min.css")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/KendoLib/js/kendo.all.min.js")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul id="menu" class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    @*<li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                    <li>@Html.ActionLink("Contact", "ContactUs", "ContactUs")</li>

                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        @*<div id="kendiInit">
            <input data-role="numerictextbox" />
            <input data-role="combobox" />

            <input data-role="button" type="button" />


            <div data-role="grid" data-source="{data: [{name: 'John Doe'},{name: 'Jane Doe'}]}"></div>

        </div>*@

        <div class="demo-section k-content">
            <div id="chart" style="background: center no-repeat url('../content/shared/styles/world-map.png');"></div>

            <div id="chart1" style="background: center no-repeat url('../content/shared/styles/world-map.png');"></div>

        </div>

        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

   
    @*@Scripts.Render("~/bundles/bootstrap")*@
   

    @*@Scripts.Render("~/AppJs/LayoutJs.js")*@








    @RenderSection("scripts", required: false)
</body>
</html>
